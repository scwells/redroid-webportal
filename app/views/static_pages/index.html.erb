  <div class="col-lg-8 col-md-8 col-sm-8" id="main-column">
    <div class="row" id="title-row">
      <%= render 'layouts/static_pages/header' %>
    </div>
    <div class="row row-spacer" id="spacer-row">
    </div>
    <div class="row" id="about-row">
      <%= render 'layouts/static_pages/about' %>
    </div>
    <div class="row row-spacer" id="spacer-row">
